<RaiseFault name="RF-InvalidUserCredentials">
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="text/plain">Incorrect Credentials</Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
