<RaiseFault name="RF-Invalid-Configuration">
  <DisplayName>Error Response: Invalid Configuration</DisplayName>
  <FaultResponse>
    <Set>
      <Headers>
        <Header name="Location">http://apigee.com/about?error=invalid_configuration&amp;error_description=Server is configured incorrectly</Header>
        <Header name="Access-Control-Allow-Origin">{request.header.origin}</Header>
        <Header name="Access-Control-Allow-Credentials">true</Header>
        <Header name="Access-Control-Allow-Methods">OPTIONS,GET,POST,PUT,DELETE,HEAD</Header>
        <Header name="Access-Control-Allow-Headers">Authorization,Content-Type,Accept,Origin,X-Requested-With,X-CSDKVersion</Header>
        <Header name="Access-Control-Max-Age">86400</Header>
      </Headers>
      <StatusCode>302</StatusCode>
      <Payload></Payload>
    </Set>
  </FaultResponse>
</RaiseFault>
