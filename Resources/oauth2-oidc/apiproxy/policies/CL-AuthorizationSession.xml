<LookupCache name="CL-AuthorizationSession">
  <DisplayName>Lookup Cache: Application Session</DisplayName>
  <CacheResource>cache1</CacheResource>
  <Scope>Global</Scope>
  <AssignTo>session.Payload</AssignTo>
  <CacheKey>
    <Prefix>authz-sess</Prefix>
    <KeyFragment ref="request.formparam.sessionid"></KeyFragment>
  </CacheKey>
</LookupCache>
